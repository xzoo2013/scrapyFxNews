#-*-coding:utf-8-*-

from scrapy.item import Item, Field

class Crawlfx168Item(Item):
    # define the fields for your item here like:
    # name = Field()
    title=Field()
    content=Field()
    tag=Field()
    url=Field()
    isShortMes=Field()
    #计划是全局新闻编号,在monogodb中的parameter  collection 中存储. 2013-7-27
    # news_id=Field()
    # 把时间与日期的字段分开存储, 方便日后的处理,新思路,2013-7-27
    dtime=Field()
    year=Field()
    month=Field()
    day=Field()
    hour=Field()
    minite=Field()
    second=Field()

